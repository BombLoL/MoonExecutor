-- Scripts:

local function LUVGTZ_fake_script() -- Script1.lol 
	local script = Instance.new('LocalScript', Script1)

	script.Parent.MouseButton1Click:Connect(function()
		local executorBox = script.Parent.Parent.Parent.Parent.Script
		
		executorBox.Text = "--MoonExecutor - Headless Script--\nlocal char = game.Players.LocalPlayer.Character\nlocal head = char.Head\nlocal face = head.face\nhead.Transparency = 1\nface.ImageTransparency = 1"
	end)
end
coroutine.wrap(LUVGTZ_fake_script)()
local function GCCR_fake_script() -- Script2.lol 
	local script = Instance.new('LocalScript', Script2)

	script.Parent.MouseButton1Click:Connect(function()
		local executorBox = script.Parent.Parent.Parent.Parent.Script
		
		executorBox.Text = "loadstring(game:HttpGet(\"https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source\"))()"
	end)
end
coroutine.wrap(GCCR_fake_script)()
