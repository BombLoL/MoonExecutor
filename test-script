local function QUVAI_fake_script() -- Execute.Execute 
	local script = Instance.new('LocalScript', Execute)

	function chunkName()
		-- Original: Infinite Yield
		-- Modded: Moon Executor
	
		local length = math.random(10,20)
		local array = {}
		for i = 1, length do
			array[i] = string.char(math.random(29, 159))
		end
		return table.concat(array)
	end
	
	local executeBox = script.Parent.Parent.Script
	
	script.Parent.MouseButton1Click:Connect(function()
		loadstring(executeBox.Text)
	end)
end
coroutine.wrap(QUVAI_fake_script)()
local function EVWU_fake_script() -- Clear.Clear 
	local script = Instance.new('LocalScript', Clear)

	local executeBox = script.Parent.Parent.Script
	
	script.Parent.MouseButton1Click:Connect(function()
		executeBox.Text = ""
	end)
end
coroutine.wrap(EVWU_fake_script)()
local function NKWXIYM_fake_script() -- AlwaysOnTop.AOT 
	local script = Instance.new('LocalScript', AlwaysOnTop)

	script.Parent.MouseButton1Click:Connect(function()
		local function checkCoreGui()
			if game.CoreGui == nil then
				return 0
			else
				return 1
			end
		end
		
		if checkCoreGui() == 0 then
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = "Moon Executor";
				Text = "CoreGui Can't be accessed, make sure your executor can access the CoreGui.";
				Duration = math.huge;
				Button1 = "OK"
			})
		elseif checkCoreGui() == 1 then
			local mainFrame = script.Parent.Parent.Parent.Parent
			if mainFrame.Parent == game.Players.LocalPlayer.PlayerGui then
				mainFrame.Parent = game.CoreGui
				script.Parent.Text = "Always On Top: True"
			else
				mainFrame.Parent = game.Players.LocalPlayer.PlayerGui
				script.Parent.Text = "Always On Top: False"
			end
		end
	end)
end
coroutine.wrap(NKWXIYM_fake_script)()
local function AAYMOFG_fake_script() -- TopBar.Dragging 
	local script = Instance.new('LocalScript', TopBar)

	function dragGUI(gui)
		local TweenService = game:GetService("TweenService")
		local UserInputService = game:GetService("UserInputService")
		
		task.spawn(function()
			local dragging
			local dragInput
			local dragStart = Vector3.new(0,0,0)
			local startPos
			local function update(input)
				local delta = input.Position - dragStart
				local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
				TweenService:Create(gui, TweenInfo.new(.20), {Position = Position}):Play()
			end
			gui.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
					dragging = true
					dragStart = input.Position
					startPos = gui.Position
	
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragging = false
						end
					end)
				end
			end)
			gui.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					dragInput = input
				end
			end)
			UserInputService.InputChanged:Connect(function(input)
				if input == dragInput and dragging then
					update(input)
				end
			end)
		end)
	end
	
	dragGUI(script.Parent)
end
coroutine.wrap(AAYMOFG_fake_script)()
local function RPCDYV_fake_script() -- Minimize.Min 
	local script = Instance.new('LocalScript', Minimize)

	script.Parent.MouseButton1Click:Connect(function()
		local main = script.Parent.Parent.Main
		
		if main.Visible == true then
			main.Visible = false
		else
			main.Visible = true
		end
	end)
end
coroutine.wrap(RPCDYV_fake_script)()
local function CUPQCYG_fake_script() -- Close.Close 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		local exec = script.Parent.Parent.Parent
		exec:Destroy()
	end)
end
coroutine.wrap(CUPQCYG_fake_script)()
local function PBXSB_fake_script() -- MoonExec.Bypasser 
	local script = Instance.new('LocalScript', MoonExec)

	function randomString()
		-- Original: Infinite Yield
		-- Modded: Moon Executor
		
		local length = math.random(10,20)
		local array = {}
		for i = 1, length do
			array[i] = string.char(math.random(29, 159))
		end
		return table.concat(array)
	end
	
	script.Parent.Name = randomString()
	script.Name = randomString()
end
coroutine.wrap(PBXSB_fake_script)()
local function EIWME_fake_script() -- MoonExec.Loadstring 
	local script = Instance.new('Script', MoonExec)

	local serverscript = game.ServerScriptService
	
	function checkLoadstring()
		if serverscript.LoadstringEnabled == false then
			return 0
		else
			return 1
		end
	end
	
	if checkLoadstring() == 0 then
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "Moon Executor",
			Text = "Loadstring isn't enabled in this game, some scripts will not work.",
			Duration = math.huge,
			Button1 = "Ok",
		})
	end
end
coroutine.wrap(EIWME_fake_script)()
